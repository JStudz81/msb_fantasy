<h1>Players</h1>

<ul>
    <% @players.each do |player| %>
        <li>
            <%= link_to player.name, player %> - <%= player.fantasy_team.team_name %>
        </li>
    <% end %>
</ul>

<h1>Fantasy Teams</h1>

<ul>
    <% @teams.each do |team| %>
        <li>
            <%= link_to team.team_name, team %>
        </li>
    <% end %>
</ul>
