<h1><%= @team.team_name %></h1>

<ul>
    <% @team.week_stats.where(week: 1).each do |weekStat| %>
        <li>
            <%= weekStat.player.name %> - This weeks points: <%= if weekStat.player.week_stats.exists? 
            weekStat.player.week_stats.order("week").last.points 
            end%>
        </li>
    <% end %>
</ul>
